{
  "name": "primap-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sue445/primap.git"
  },
  "scripts": {
    "clean": "rm -rf ./node_modules package-lock.json && npm i",
    "upgrade": "npm install typescript@latest react@latest react-dom@latest @types/react@latest @types/react-dom@latest webpack@latest webpack-dev-server@latest webpack-cli@latest ts-loader@latest clean-webpack-plugin@latest html-webpack-plugin@latest --save-exact",
    "build": "webpack",
    "start": "webpack serve",
    "test": "jest",
    "prettier:base": "prettier --config prettier.config.js",
    "prettier:check": "npm run prettier:base -- --list-different \"**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"**/*.{ts,tsx}\""
  },
  "devDependencies": {
    "@types/googlemaps": "^3.40.3",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "autoprefixer": "^10.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.0",
    "css-loader": "^5.0.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.0",
    "postcss": "^8.1.6",
    "postcss-loader": "^4.0.4",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.9",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@sentry/react": "^5.27.3",
    "@sentry/tracing": "^5.27.3",
    "firebase": "^7.24.0",
    "geofirex": "^0.1.0",
    "google-maps-react": "^2.0.6",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rxjs": "^6.6.3",
    "tailwindcss": "^1.9.6"
  }
}
