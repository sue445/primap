{
  "name": "primap-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sue445/primap.git"
  },
  "scripts": {
    "clean": "rm -rf ./node_modules package-lock.json && npm i",
    "upgrade": "npm install typescript@latest react@latest react-dom@latest @types/react@latest @types/react-dom@latest webpack@latest webpack-dev-server@latest webpack-cli@latest ts-loader@latest clean-webpack-plugin@latest html-webpack-plugin@latest --save-exact",
    "build": "webpack",
    "start": "webpack serve",
    "test": "jest",
    "prettier:base": "prettier --config prettier.config.js",
    "prettier:check": "npm run prettier:base -- --list-different \"**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"**/*.{ts,tsx}\""
  },
  "devDependencies": {
    "@types/googlemaps": "^3.43.3",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.2",
    "autoprefixer": "^10.4.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.2",
    "prop-types": "^15.8.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.9",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "dependencies": {
    "@sentry/react": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "firebase": "^7.24.0",
    "geofirex": "^0.1.0",
    "google-maps-react": "^2.0.6",
    "react": "^18.0.0",
    "react-dom": "^18.1.0",
    "rxjs": "^7.5.5",
    "tailwindcss": "^3.0.24"
  }
}
